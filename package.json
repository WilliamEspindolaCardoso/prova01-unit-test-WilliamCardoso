{
  "name": "unit-tests-jest",
  "version": "1.0.0",
  "description": "Example to unit tests using Jest",
  "main": "index.js",
  "author": "ugioni",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf ./coverage && mkdirp ./coverage",
    "test": "npm run clean && jest",
    "coverage": "npm run clean && jest --coverage"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WilliamEspindolaCardoso/unit-tests-will.git"
  },
  "keywords": [],
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/WilliamEspindolaCardoso/unit-tests-will/issues"
  },
  "homepage": "https://github.com/WilliamEspindolaCardoso/unit-tests-will#readme",
  "devDependencies": {
    "jest": "^30.1.2",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.0.1"
  }
}
